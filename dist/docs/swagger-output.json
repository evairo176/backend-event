{
    "openapi": "3.0.0",
    "info": {
        "version": "1.0.0",
        "title": "Dokumentasi API acara",
        "description": "Dokumentasi API untuk acara"
    },
    "servers": [
        {
            "url": "http://localhost:8000/api/v1",
            "description": "Local server"
        },
        {
            "url": "https://backend-event-two.vercel.app/api/v1",
            "description": "Production server"
        }
    ],
    "paths": {
        "/register": {
            "post": {
                "tags": ["Auth"],
                "description": "",
                "responses": {
                    "default": {
                        "description": ""
                    }
                },
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RegisterRequest"
                            }
                        },
                        "application/xml": {
                            "schema": {
                                "$ref": "#/components/schemas/RegisterRequest"
                            }
                        }
                    }
                }
            }
        },
        "/login": {
            "post": {
                "tags": ["Auth"],
                "description": "",
                "responses": {
                    "default": {
                        "description": ""
                    }
                },
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginRequest"
                            }
                        },
                        "application/xml": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginRequest"
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer"
            }
        },
        "schemas": {
            "LoginRequest": {
                "type": "object",
                "properties": {
                    "identifier": {
                        "type": "string",
                        "example": "agung2025"
                    },
                    "password": {
                        "type": "string",
                        "example": "Agung2025!"
                    }
                },
                "xml": {
                    "name": "LoginRequest"
                }
            },
            "RegisterRequest": {
                "type": "object",
                "properties": {
                    "fullName": {
                        "type": "string",
                        "example": "member2025"
                    },
                    "username": {
                        "type": "string",
                        "example": "member2025"
                    },
                    "email": {
                        "type": "string",
                        "example": "member2025@yopmail.com"
                    },
                    "password": {
                        "type": "string",
                        "example": "Member2025!"
                    },
                    "confirmPassword": {
                        "type": "string",
                        "example": "Member2025!"
                    }
                },
                "xml": {
                    "name": "RegisterRequest"
                }
            },
            "ActivationRequest": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string",
                        "example": "abcdef"
                    }
                },
                "xml": {
                    "name": "ActivationRequest"
                }
            },
            "UpdateProfileRequest": {
                "type": "object",
                "properties": {
                    "fullName": {
                        "type": "string",
                        "example": ""
                    },
                    "profilePicture": {
                        "type": "string",
                        "example": ""
                    }
                },
                "xml": {
                    "name": "UpdateProfileRequest"
                }
            },
            "UpdatePasswordRequest": {
                "type": "object",
                "properties": {
                    "oldPassword": {
                        "type": "string",
                        "example": ""
                    },
                    "password": {
                        "type": "string",
                        "example": ""
                    },
                    "confirmPassword": {
                        "type": "string",
                        "example": ""
                    }
                },
                "xml": {
                    "name": "UpdatePasswordRequest"
                }
            },
            "CreateCategoryRequest": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "example": ""
                    },
                    "description": {
                        "type": "string",
                        "example": ""
                    },
                    "icon": {
                        "type": "string",
                        "example": ""
                    }
                },
                "xml": {
                    "name": "CreateCategoryRequest"
                }
            },
            "CreateEventRequest": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "example": ""
                    },
                    "banner": {
                        "type": "string",
                        "example": "fileUrl"
                    },
                    "category": {
                        "type": "string",
                        "example": "category ObjectID"
                    },
                    "description": {
                        "type": "string",
                        "example": ""
                    },
                    "startDate": {
                        "type": "string",
                        "example": "yyyy-mm-dd hh:mm:ss"
                    },
                    "endDate": {
                        "type": "string",
                        "example": "yyyy-mm-dd hh:mm:ss"
                    },
                    "location": {
                        "type": "object",
                        "properties": {
                            "region": {
                                "type": "number",
                                "example": 3273
                            },
                            "coordinates": {
                                "type": "array",
                                "example": [0, 0],
                                "items": {
                                    "type": "number"
                                }
                            },
                            "address": {
                                "type": "string",
                                "example": ""
                            }
                        }
                    },
                    "isOnline": {
                        "type": "boolean",
                        "example": false
                    },
                    "isFeatured": {
                        "type": "boolean",
                        "example": false
                    },
                    "isPublish": {
                        "type": "boolean",
                        "example": false
                    }
                },
                "xml": {
                    "name": "CreateEventRequest"
                }
            },
            "RemoveMediaRequest": {
                "type": "object",
                "properties": {
                    "fileUrl": {
                        "type": "string",
                        "example": ""
                    }
                },
                "xml": {
                    "name": "RemoveMediaRequest"
                }
            },
            "CreateBannerRequest": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string",
                        "example": "Banner 3 - Title"
                    },
                    "image": {
                        "type": "string",
                        "example": "https://res.cloudinary.com/five-code/image/upload/v1734918925/f70vpihmblj8lvdmdcrs.png"
                    },
                    "isShow": {
                        "type": "boolean",
                        "example": false
                    }
                },
                "xml": {
                    "name": "CreateBannerRequest"
                }
            },
            "CreateTicketRequest": {
                "type": "object",
                "properties": {
                    "price": {
                        "type": "number",
                        "example": 1500
                    },
                    "name": {
                        "type": "string",
                        "example": "Ticket Reguler"
                    },
                    "events": {
                        "type": "string",
                        "example": "6762aa5dacb76a9b3e2cb1da"
                    },
                    "description": {
                        "type": "string",
                        "example": "Ticket Reguler - Description"
                    },
                    "quantity": {
                        "type": "number",
                        "example": 100
                    }
                },
                "xml": {
                    "name": "CreateTicketRequest"
                }
            },
            "CreateOrderRequest": {
                "type": "object",
                "properties": {
                    "events": {
                        "type": "string",
                        "example": "event object id"
                    },
                    "ticket": {
                        "type": "string",
                        "example": "ticket object id"
                    },
                    "quantity": {
                        "type": "number",
                        "example": 1
                    }
                },
                "xml": {
                    "name": "CreateOrderRequest"
                }
            }
        }
    }
}
